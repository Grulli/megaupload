<%= form_for @up_file, :html => { :multipart => true } do |f| %>
  <% if @up_file.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@up_file.errors.count, "error") %> evit√≥ a este archivo ser guardado.:</h2>

      <ul>
      <% @up_file.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :Url %><br />
    <%= f.text_field :Url %>
  </div>
  <div class="field">
    <%= f.label :mail %><br />
    <%= f.text_field :mail %>
  </div>
  <div class="field">
    <%= f.label :upload_file %><br />
    <%= f.file_field :upload_file %>
